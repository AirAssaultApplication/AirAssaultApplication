{
    "cli": {
      "promptToConfigurePushNotifications": false,
      "appVersionSource": "remote" //"local" will store version based on builds
    },
    "build": {
      //Android V1
      "preview": {
        "extends": "production",
        "distribution": "internal",
        "android": {
          "buildType": "apk"
        }
      },
      //untested
      "development": {
        "extends": "production",
        "distribution": "internal"
      },
      //untested
      "preview2": {
        "extends": "production",
        "android": {
          "gradleCommand": ":app:assembleRelease"
        }
      },
      //iOS V1
      "iOS": {
        "extends": "production",
        "distribution": "internal"
      },
      //version incrementer
      "production": {
        "autoIncrement": true
      }
    }
  }
  